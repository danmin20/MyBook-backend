    "@babel/runtime": "^7.8.4",
    "@sendgrid/mail": "^6.5.1",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "graphql-tools": "^4.0.6",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.7.6",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.4.2",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "prisma": "^1.34.10",
    "prisma-client-lib": "^1.34.10"
